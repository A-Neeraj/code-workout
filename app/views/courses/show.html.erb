<div class="container">
	<div id="heading">
		<div class="row col-lg-9">
			<h1>Course Title</h1>
		</div>
	</div>

	<br>
	</br>

	<div id="sections">

		<div class="row">

			<div class="col-lg-3 text-center">
				<strong>Sections</strong>
			</div>
			<div class="col-lg-3 text-center">
				<strong>Teachers</strong>
			</div>
			<div class="col-lg-3 text-center">
				<strong>Graders</strong>
			</div>

		</div>

		<div class="row">

			<div class="col-lg-3 text-center">
				CRN 12345
			</div>

			<div class="col-lg-3 text-center">
				Abcde
			</div>
			<div class="col-lg-3 text-center">
				Al,Go
			</div>

		</div>

		<div class="row">

			<div class="col-lg-3 text-center">
				CRN 56789
			</div>
			<div class="col-lg-3 text-center">
				Pyth
			</div>
			<div class="col-lg-3 text-center">
				Styl
			</div>

		</div>

	</div>
	<br>
	</br>

	<div id="tabs">

		<ul class="nav nav-tabs">
			<li class="active col-lg-3">
				<a href="#workouts">Workouts</a>
			</li>
			<li class="col-lg-3">
				<a href="#excercises">Excercices</a>
			</li>
			<li class="col-lg-3">
				<a href="#grades">Grades</a>
			</li>
			<li class="col-lg-3">
				<a href="#roster">Roster</a>
			</li>
		</ul>

		<div class="tab-content">
			<div class="tab-pane active" id="workouts">
				<%= render "courses/workouts" %>
			</div>
			<div class="tab-pane" id="excercises">
				The excercise bank for this course
			</div>
			<div class="tab-pane" id="grades">
				Grades for this course
			</div>
			<div class="tab-pane" id="roster">
				Roster for this course
			</div>
		</div>
	</div>

</div>
<div style="width: 100%; height: 60px; clear: both;"></div>

<script type="text/javascript">
	$(function() {
		$('#tabs a').click(function(e) {
			e.preventDefault();
			$(this).tab('show');
		});

	}); 
</script>